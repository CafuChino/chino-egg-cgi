{
  "name": "chino-egg-cgi",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-chino-egg-cgi",
    "stop": "egg-scripts stop --title=egg-server-chino-egg-cgi",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "cross-env egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "cross-env eslint . && cross-env NODE_ENV=test npx sequelize db:migrate && egg-bin cov",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "plop": "^2.7.4"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "factory-girl": "^5.0.4",
    "sequelize-cli": "^6.2.0",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
